      \u6211\u5C31\u8B80\u7684\u5B78\u6821\u2014\u2014\u79C1\u7ACB\u4E0A\u7E3D\u5712\u5B78\u5712\u570B\u4E2D\u90E8\u3002\r\nboku: 
      \u807D\u8AAA\u9019\u5EA7\u4EE5\u74E6\u7247\u6253\u9020\u7684\u96C4\u5049\u6821\u820D\uFF0C\u662F\u5341\u5E74\u524D\u5F9E\u6B50\u6D32\u67D0\u500B\u570B\u5BB6\u79FB\u5EFA\u904E\u4F86\u7684(\u539F\u672C\u597D\u50CF\u662F\u9AD8\u4E2D)\u3002\r\nboku: 
      \u4ECA\u5E74\u56DB\u6708\uFF0C\u6211\u525B\u8E0F\u5165\u6821\u5712\u6642\u7ACB\u523B\u5C31\u88AB\u9019\u6709\u8457\u6B50\u98A8\u7684\u838A\u56B4\u5EFA\u7BC9\u7D66\u9707\u61FE\uFF0C\u6211\u662F\u771F\u7684\u6253\u5F9E\u5FC3\u5E95\u8D0A\u5606\u9019\u68DF\u7F8E\u9E97\u7684\u5EFA\u7BC9\u3002\r\nboku: 
      \u4F46\u662F\uFF0C\u5EA7\u843D\u65BC\u9019\u838A\u56B4\u6821\u820D\u6B63\u4E2D\u592E\u7684\u9418\u5854\u7ADF\u7136\u58DE\u6389\u4E86\uFF0C\u90A3\u8457\u5BE6\u662F\u500B\u715E\u98A8\u666F\u7684\u5B58\u5728\u3002\r\nboku: 
      \u5F9E\u6211\u56E0\u5931\u671B\u800C\u5E36\u8457\u4E9B\u8A31\u4E0D\u6EFF\u7684\u89C0\u9EDE\u4F86\u770B\uFF0C\u9019\u5EA7\u9418\u5854\u7684\u5916\u89C0\u5C31\u50CF\u662F\u4E00\u500B\u666E\u901A\u7684\u74E6\u9020\u9577\u65B9\u9AD4\uFF0C\u9019\u771F\u7684\u662F\u4EF6\u795E\u5947\u7684\u4E8B\u3002\r\nboku: 
      \u5B83\u7D66\u4EBA\u7684\u5370\u8C61\uFF0C\u8207\u5176\u8AAA\u662F\u9418\u5854\uFF0C\u5012\u4E0D\u5982\u8AAA\u50CF\u4E0A\u982D\u63D2\u4E86\u4E00\u6839\u7D30\u91DD\u7684\u56DB\u89D2\u7159\u56EA\uFF0C\u800C\u5B83\u5927\u6982\u4E5F\u53EA\u80FD\u5728\u8056\u8A95\u7BC0\u671F\u9593\u6D3E\u4E0A\u7528\u5834\u5427\u3002\r\nboku: 
      \u4E0D\u904E\u5B83\u4E26\u4E0D\u662F\u7159\u56EA\uFF0C\u7576\u7136\u4E5F\u4E0D\u80FD\u5728\u8056\u8A95\u7BC0\u767C\u63EE\u4F5C\u7528\u3002\r\nboku: 
      \u6211\u525B\u9032\u5B78\u6821\u7684\u6642\u5019\u6642\u9418\u9084\u6C92\u6545\u969C\u3002\r\nboku: 
      \u4F46\u5927\u6982\u5728\u4E94\u6708\u7684\u6642\u5019\uFF0C\u5B83\u7684\u5206\u91DD\u5C31\u4E00\u76F4\u505C\u5728\u300CX\u300D\u7684\u4F4D\u7F6E\u4E0A\uFF0C\u534A\u5E74\u4F86\u6C92\u4EBA\u4F86\u4FEE\u7406\uFF0C\u4E00\u76F4\u653E\u4EFB\u5230\u73FE\u5728\u3002\r\nboku: 
      \u6C92\u53BB\u4FEE\u7406\u7684\u539F\u56E0\u4E26\u4E0D\u662F\u56E0\u70BA\u5B83\u6545\u969C\u7684\u554F\u984C\u5F88\u5FA9\u96DC\u2014\u2014\u807D\u8AAA\u5B78\u6821\u65B9\u9762\u8A8D\u70BA\u90A3\u53EA\u662F\u5C0F\u6545\u969C\u5C31\u6C92\u53BB\u7BA1\u5B83\u3002\r\nboku: 
      \u6211\u7576\u6642\u9084\u62B1\u6301\u8457\u6216\u8A31\u5728\u6691\u5047\u5C31\u6703\u6709\u4EBA\u4F86\u4FEE\u7684\u60F3\u6CD5\uFF0C\u4E0D\u904E\u4E26\u975E\u5982\u6B64\u3002\r\nboku: 
      \u7576\u7B2C\u4E8C\u5B78\u671F\u958B\u59CB\u6642\uFF0C\u6642\u9418\u4F9D\u7136\u662F\u6545\u969C\u7684\u3002\r\nboku: 
      \u6211\u4E0D\u7981\u60F3\u5927\u53EB\uFF0C\u4FEE\u500B\u6642\u9418\u5230\u5E95\u6703\u6709\u591A\u9EBB\u7169\u554A\uFF01\u4E0D\u904E\uFF0C\u7AD9\u5728\u5B78\u6821\u7684\u7ACB\u5834(\u96D6\u7136\u6015\u9EBB\u7169\u5927\u6982\u662F\u4E0D\u722D\u7684\u4E8B\u5BE6)\uFF0C\u9802\u8457\u79C1\u7ACB\u540D\u6821\u7684\u982D\u929C\uFF0C\u4ED6\u5011\u5927\u6982\u4E0D\u6703\u8F15\u6613\u8B93\u6821\u5916\u4EBA\u58EB(\u6307\u4FEE\u7406\u5EE0\u5546)\u9032\u51FA\u6821\u5712\u5427\u3002\r\nboku: 
      \u4E0D\u904E\u5728\u4E0B\u5B78\u5E74\u65B0\u751F\u5165\u5B78\u524D\u5915\u6559\u8077\u54E1\u5011\u5927\u6982\u6703\u6709\u4E00\u4E9B\u52D5\u4F5C\u5427\u3002\r\nboku: 
      \u5728\u9019\u4E4B\u524D\u4ED6\u5011\u5927\u6982\u6703\u62B1\u8457\u300C\u904B\u6C23\u597D\u7684\u8A71\uFF0C\u8AAA\u4E0D\u5B9A\u5B83\u53C8\u6703\u81EA\u5DF1\u52D5\u4E86\u300D\u7684\u5FC3\u614B\u975C\u89C0\u5176\u8B8A\u3002\r\nboku: 
      \u518D\u8AAA\uFF0C\u90A3\u500B\u6642\u9418\u672C\u4F86\u5C31\u662F\u5C6C\u65BC\u5916\u570B\u5EFA\u7BC9\uFF0C\u5982\u679C\u96A8\u4FBF\u4E82\u52D5\u8AAA\u4E0D\u5B9A\u53CD\u800C\u6703\u9020\u6210\u7121\u53EF\u633D\u6551\u7684\u4E0B\u5834\u3002\r\nboku: 
      \u96D6\u7136\u6211\u525B\u624D\u66FE\u8AAA\u904E\u9019\u6A23\u6709\u9EDE\u300C\u53EF\u60DC\u300D\uFF0C\u4F46\u4E8B\u5BE6\u4E0A\uFF0C\u6211\u4E00\u9EDE\u90FD\u4E0D\u89BA\u5F97\u9418\u5854\u6545\u969C\u4E86\u6709\u4EC0\u9EBC\u53EF\u60DC\u4E4B\u8655\u3002\r\nboku: 
      \u6545\u969C\u7684\u9418\u5854\u58DE\u4E86\u6B50\u98A8\u6821\u820D\u7684\u838A\u56B4\u6C23\u606F\u2014\u2014\u5176\u5BE6\u6211\u4E26\u4E0D\u8A0E\u53AD\u9019\u6A23\u5931\u8861\u7684\u72C0\u6CC1\u3002\r\nboku: 
      \u96D6\u7136\u6703\u89BA\u5F97\u5931\u671B\uFF0C\u4F46\u662F\u9019\u4EFD\u5931\u671B\u537B\u53EB\u4EBA\u8208\u596E\u4E0D\u5DF2\u3002\r\nboku: 
      \u9019\u7A2E\u4E0D\u5B89\u5B9A\u3001\u4F3C\u4E4E\u6709\u4EC0\u9EBC\u5C07\u8981\u767C\u751F\u7684\u72C0\u614B\u3002\r\nboku: 
      \u975E\u5E38\u614B\u7684\u4E8B\u7269\u7E3D\u662F\u5E36\u7D66\u4EBA\u65B0\u5947\u8207\u8208\u596E\u2014\u2014\u96D6\u7136\u5728\u73FE\u5BE6\u751F\u6D3B\u4E2D\uFF0C\u5340\u5340\u4E00\u6839\u6821\u9418\u4E0A\u7684\u6307\u91DD\u505C\u4E86\u4E26\u4E0D\u80FD\u5F15\u767C\u4EC0\u9EBC\u9A5A\u5929\u52D5\u5730\u7684\u5927\u4E8B\u3002\r\nboku: 
      \u4E0D\u904E\u65E2\u7136\u9023\u8774\u8776\u62CD\u7FC5\u90FD\u80FD\u5F15\u767C\u9F8D\u5377\u98A8\uFF0C\u5C0F\u5C0F\u4E00\u500B\u5984\u60F3\u61C9\u8A72\u7121\u50B7\u5927\u96C5\u5427\u3002\r\nboku: 
      \u62D0\u5F4E\u62B9\u89D2\u5730\u7E5E\u4E86\u4E00\u5927\u5708\uFF0C\u6211\u60F3\u8AAA\u7684\u5176\u5BE6\u662F\uFF0C\u5982\u679C\u4F60\u76F8\u4FE1\u4E0A\u7E3D\u5712\u5B78\u5712\u9418\u5854\u4E0A\u6240\u986F\u793A\u7684\u6642\u9593\uFF0C\u4F60\u5C31\u6709\u82E6\u982D\u5403\u4E86\u3002\r\nboku: 
      \u4F60\u5FC5\u9808\u7121\u8996\u5206\u91DD\u7684\u5B58\u5728\uFF0C\u53EA\u770B\u8457\u6642\u91DD\uFF0C\u5C31\u50CF\u89C0\u5BDF\u65E5\u6677\u822C\u2014\u2014\u73FE\u5728\u6642\u523B\uFF0C\u5927\u7D04\u4E0B\u5348\u56DB\u9EDE\u5341\u4E94\u5206\u5DE6\u53F3\u3002\r\nboku: 
      \u5728\u5357\u6821\u820D\u4E00\u6A13\u7684\u5716\u66F8\u9928\u5167\uFF0C\u6211\u671B\u5411\u7A97\u5916\u770B\u8457\u9418\u5854\u60F3\u9019\u4E9B\u6F2B\u7121\u908A\u969B\u7684\u4E8B\u3002\r\nboku: 
      \u6B63\u662F\u653E\u5B78\u6642\u523B\u3002\r\nboku:  \u6211\u6B63\u5728\u95B1\u8B80\u4E00\u672C\u66F8\u3002\r\nboku: 
      \u66F8\u540D\u662F\u300A\u5C07\u68CB\u8F15\u677E\u5165\u9580\u5C0F\u5B78\u751F\u5C08\u7528\u300B\u3002\r\nboku: 
      \u6211\u4E0D\u77E5\u9053\u70BA\u4EC0\u9EBC\u570B\u4E2D\u7684\u5716\u66F8\u5BA4\u88E1\u6703\u6709\u5C0F\u5B78\u751F\u7684\u66F8\uFF0C\u4F46\u5149\u662F\u60F3\u50CF\u4E00\u500B\u570B\u4E2D\u751F\u9084\u5728\u770B\u5C0F\u5B78\u751F\u7528\u7684\u5C07\u68CB\u5165\u9580\u66F8\uFF0C\u5C31\u80FD\u524C\u6FC0\u6211\u88AB\u6295\u4EE5\u5947\u7570\u76EE\u5149\u7684\u81EA\u8650\u5FEB\u611F\u3002\r\nboku: 
      \u6240\u4EE5\u6211\u624D\u7121\u8996\u65BC\u5176\u4ED6\u7684\u5C07\u68CB\u5165\u9580\u66F8\uFF0C\u800C\u6311\u4E86\u624B\u4E2D\u9019\u672C\u3002\r\nboku: 
      \u4E0D\u53BB\u95B1\u8B80\u67B6\u4E0A\u9019\u7A2E\u6975\u5177\u6311\u6230\u6027\u7684\u66F8\u672C\uFF0C\u6211\u5C31\u4E0D\u662F\u6211\u4E86\u3002\r\nboku: 
      \u96D6\u7136\u5716\u66F8\u5BA4\u5167\u4EBA\u6F6E\u4E26\u4E0D\u591A\uFF0C\u4F46\u611F\u89BA\u4ED6\u5011\u7684\u8996\u7DDA\u5168\u96C6\u4E2D\u5728\u6211\u8EAB\u4E0A\r\nboku: 
      \u8D85\u8208\u596E\u7684\u3002\r\nboku:  \u554A\u554A\uFF0C\u81EA\u6211\u610F\u8B58\u904E\u65BC\u5F37\u70C8\u597D\u6709\u8DA3\u5594\uFF01
      \r\nboku:  \u2026\u2026\u4E0D\u904E\uFF0C\u96D6\u7136\u6211\u7684\u66F8\u5305\u4E0D\u50CF\u5C0F\u5B78\u66F8\u5305\uFF0C\u4F46\u6211\u73FE\u5728\u5076\u723E\u9084\u662F\u6703\u628A\u6578\u5B78\u5BEB\u6210\u7B97\u6578\u3002\r\n\r\nboku: 
      \u300C\u55EF\u2014\u2014\u300D \r\n \r\nboku:  \u7E3D\u4E4B\uFF0C\u5728(\u4E8B\u5BE6\u4E0A\u6839\u672C\u4E0D\u5B58\u5728\u7684)\u89C0\u773E\u6CE8\u8996\u4E0B\uFF0C\u6211\u662F\u4E00\u500B\u611B\u6F14\u6232\u3001\u71B1\u5FC3\u670D\u52D9\u89C0\u773E\u7684\u7537\u5B69\u5B50\u3002\u4E0D\u904E\uFF0C\u539F\u4F86\u5982\u6B64\u554A\uFF0C\u89D2\u884C(\u6CE82)\u5538\u300CKAKUGYOU\u300D\u2026\u2026\u96E3\u602A\u6BCF\u6B21\u6211\u8AAA\u300CKAKKOU\u300D\u3001\u300CKAKKOU\u300D\u7684\u6642\u5019\uFF0C\u75C5\u9662\u962A\u5B78\u59CA\u90FD\u6703\u4E00\u526F\u597D\u7B11\u7684\u8868\u60C5\u3002
      \r\n\r\nboku:  \u53EF\u662F\uFF0C\u70BA\u4EC0\u9EBC\u6BCF\u4E00\u96BB\u68CB\u4E00\u9032\u5230\u6575\u65B9\u7684\u9663\u5730\u5C31\u53EF\u4EE5\u8D70\u91D1\u5C07\u7684\u8D70\u6CD5\u5462\uFF1F\u9664\u4E86\u98DB\u8ECA\u548C\u89D2\u884C\u5916\u90FD\u662F\u9019\u6A23\u3002\u6211\u4E0D\u8A8D\u70BA\u91D1\u662F\u9019\u9EBC\u597D\u7528\u7684\u68CB\u5B50\u8036\u2026\u2026\u6211\u8A18\u5F97\u897F\u6D0B\u68CB\u7684\u5175\u79FB\u5230\u6575\u65B9\u7684\u6700\u5F8C\u4E00\u6392\u6642\uFF0C\u53EF\u4EE5\u5347\u7D1A\u70BA\u738B\u4EE5\u5916\u7684\u68CB\u5B50\u5427\uFF1F\u5982\u679C\u5C07\u68CB\u4E5F\u6709\u9019\u7A2E\u7CFB\u7D71\u5C31\u597D\u4E86\u3002\u9019\u7A2E\u5347\u7D1A\u65B9\u6CD5\u592A\u72AF\u898F\u4E86\uFF0C\u800C\u4E14\u4E00\u9EDE\u8B8A\u5316\u4E5F\u6C92\u6709\u3002
      \r\n\r\nboku:  \u4E0D\u904E\u9019\u672C\u5165\u9580\u66F8\u7684\u8AAA\u660E\u9084\u771F\u7C21\u55AE\u3002
